package com.msavaliadorcredito.api.domain.model;

import java.util.List;

import lombok.Builder;

@Builder
public class SituacaoCliente {

	private DadosCliente cliente;
	private List<CartoesCliente> cartoes;
	
	
	public SituacaoCliente(DadosCliente cliente, List<CartoesCliente> cartoes) {
		super();
		this.cliente = cliente;
		this.cartoes = cartoes;
	}
	
	
	public SituacaoCliente() {
		super();
	}


	public DadosCliente getCliente() {
		return cliente;
	}
	public void setCliente(DadosCliente cliente) {
		this.cliente = cliente;
	}
	public List<CartoesCliente> getCartoes() {
		return cartoes;
	}
	public void setCartoes(List<CartoesCliente> cartoes) {
		this.cartoes = cartoes;
	}


	
	
	
	
	
	
	
}
